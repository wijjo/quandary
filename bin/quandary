"""Quandary "solver"."""


def quandary_main():
    import sys
    import os
    root_folder = os.path.dirname(os.path.dirname(__file__))
    if root_folder not in sys.path:
        sys.path.insert(0, root_folder)
    from quandary.main import main
    main()


if __name__ == '__main__':
    quandary_main()
